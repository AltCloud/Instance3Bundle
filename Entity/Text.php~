<?php

namespace AltCloud\Instance3Bundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * AltCloud\Instance3Bundle\Entity\Text
 */
class Text
{
    /**
     * @var integer $id
     */
    private $id;

    /**
     * @var text $content
     */
    private $content;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set content
     *
     * @param text $content
     */
    public function setContent($content)
    {
        $this->content = $content;
    }

    /**
     * Get content
     *
     * @return text 
     */
    public function getContent()
    {
        return $this->content;
    }
}